{
    "name": "floeproject.org",
    "version": "2.1.0",
    "description": "FLOE Project Website",
    "scripts": {
        "build": "npm-run-all -l clean build:eleventy",
        "build:eleventy": "cross-env NODE_ENV=production eleventy",
        "clean": "rimraf dist",
        "dev": "cross-env NODE_ENV=development eleventy --serve",
        "lint": "fluid-lint-all",
        "start": "npm-run-all -l clean -p start:eleventy",
        "start:eleventy": "run-p dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluid-project/floeproject.org.git"
    },
    "keywords": [
        "Floe",
        "Project",
        "Flexible",
        "Learning",
        "Open",
        "Education"
    ],
    "author": "OCAD University <idrc@ocadu.ca>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/fluid-project/floeproject.org/issues"
    },
    "homepage": "https://github.com/fluid-project/floeproject.org#readme",
    "browserslist": [
        "defaults",
        "not IE 11",
        "not OperaMini all"
    ],
    "dependencies": {
        "@11ty/eleventy": "2.0.1",
        "@11ty/eleventy-navigation": "0.3.5",
        "@11ty/eleventy-plugin-rss": "1.2.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
        "eleventy-plugin-fluid": "1.1.0",
        "infusion": "4.6.0"
    },
    "devDependencies": {
        "cross-env": "7.0.3",
        "eleventy-plugin-sharp": "0.1.1",
        "esbuild": "0.18.11",
        "fluid-lint-all": "1.2.9",
        "image-size": "1.0.2",
        "linkedom": "0.14.26",
        "npm-run-all": "4.1.5",
        "rimraf": "5.0.1"
    }
}
